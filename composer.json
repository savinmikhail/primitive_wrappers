{
    "name": "savinmikhail/primitive_wrappers",
    "description": "The object wrappers and static helpers for php primitive types",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Mikhail\\PrimitiveWrappers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mikhail\\Tests\\PrimitiveWrappers\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Savin Mikhail",
            "email": "mikhail.d.savin@gmail.com"
        }
    ],
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "phpro/grumphp": "^2.5",
        "squizlabs/php_codesniffer": "^3.9",
        "phpmd/phpmd": "^2.15",
        "phan/phan": "^5.4"
    },
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
